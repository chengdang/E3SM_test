    </entry>
    <entry id="CIMEROOT" value="/global/u2/c/cdang/E3SM_code_v1_mdf/20180420_SW_snicar/cime">
      <type>char</type>
      <desc>full pathname of CIME source root directory</desc>
    </entry>
    <entry id="SRCROOT" value="/global/u2/c/cdang/E3SM_code_v1_mdf/20180420_SW_snicar">
      <type>char</type>
      <desc>full pathname of source root directory</desc>
    </entry>
    <entry id="CASE" value="exp.SnicarFalse.cmip6-picontrol.nodeS">
